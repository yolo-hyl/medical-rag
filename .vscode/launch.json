{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python 调试程序: 当前文件",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "标注数据",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/annotation.py",
            "console": "integratedTerminal",
            "args": [
                "/home/weihua/medical-rag/src/MedicalRag/config/data/annotator.yaml"
            ]
        },
        {
            "name": "构建稀疏向量词表",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/build_vocab.py",
            "console": "integratedTerminal"
        },
        {
            "name": "创建collection pipline",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/create_pipline.py",
            "console": "integratedTerminal",
            "args": [
                "--force-recreate"
            ]
        },
        {
            "name": "入库pipline",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/insert_data_to_collection.py",
            "console": "integratedTerminal",
            "args": [
                "--search-config", "/home/weihua/medical-rag/src/MedicalRag/config/search/search_answer.yaml"
            ]
        },
        {
            "name": "查询 pipeline",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/search_pipline.py",
            "console": "integratedTerminal",
            "args": [
                "--search-config", "/home/weihua/medical-rag/src/MedicalRag/config/search/search_answer.yaml"
            ]
        }
    ]
}